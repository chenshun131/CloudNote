<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!-- 数据库驱动.这里指定的是MAVEN本地仓库中的JAR文件-->
    <classPathEntry
            location="/Users/mew/.m2/repository/mysql/mysql-connector-java/5.1.9/mysql-connector-java-5.1.9.jar"/>

    <!-- id:数据库方言 -->
    <context id="mysqlGen" targetRuntime="MyBatis3">
        <property name="javaFileEncoding" value="UTF-8"/>

        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <!-- 是否去除自动生成的注释.true:是;false:否 自动生成代码注释是很反人类的,通常关闭注释 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <!-- 数据库链接URL、用户名、密码 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/CloudNote"
                        userId="root" password="123456">
        </jdbcConnection>

        <!--
        default false
        Java type resolver will always use java.math.BigDecimal if the database column is of type DECIMAL or NUMERIC.
        -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <!-- 生成DOMAIN的包名和位置 -->
        <!-- 指定包名以及生成的目标地址(可以自定义地址,但是路径不存在不会自动创建 .使用MAVEN将默认生成在target目录下,会自动创建路径)-->
        <javaModelGenerator targetPackage="com.chenshun.studyapp.domain" targetProject="MAVEN">
            <property name="enableSubPackages" value="false"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <!-- 生成映射文件的包名和位置-->
        <sqlMapGenerator targetPackage="mappers" targetProject="MAVEN">
            <property name="enableSubPackages" value="false"/>
        </sqlMapGenerator>

        <!-- 生成DAO的包名和位置 -->
        <javaClientGenerator type="XMLMAPPER" targetPackage="com.chenshun.studyapp.dao" targetProject="MAVEN">
            <property name="enableSubPackages" value="false"/>
        </javaClientGenerator>

        <!-- 要生成的数据库表.MYSQL可以加入主键自增、字段命名、忽略某字段等 -->
        <table tableName="cn_user" domainObjectName="User"
               enableUpdateByPrimaryKey="false" enableDeleteByPrimaryKey="false"
               enableSelectByPrimaryKey="false" enableCountByExample="false"
               enableUpdateByExample="false" enableDeleteByExample="false"
               enableSelectByExample="false" selectByExampleQueryId="false"></table>
        <table tableName="cn_notebook_type" domainObjectName="NoteBookType"
               enableUpdateByPrimaryKey="false" enableDeleteByPrimaryKey="false"
               enableSelectByPrimaryKey="false" enableCountByExample="false"
               enableUpdateByExample="false" enableDeleteByExample="false"
               enableSelectByExample="false" selectByExampleQueryId="false"></table>
        <table tableName="cn_notebook" domainObjectName="NoteBook"
               enableUpdateByPrimaryKey="false"
               enableDeleteByPrimaryKey="false" enableSelectByPrimaryKey="false"
               enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="cn_note" domainObjectName="Note"
               enableUpdateByPrimaryKey="false"
               enableDeleteByPrimaryKey="false" enableSelectByPrimaryKey="false"
               enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="cn_share" domainObjectName="Share"
               enableUpdateByPrimaryKey="false"
               enableDeleteByPrimaryKey="false" enableSelectByPrimaryKey="false"
               enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="cn_activity" domainObjectName="Activity"
               enableUpdateByPrimaryKey="false"
               enableDeleteByPrimaryKey="false" enableSelectByPrimaryKey="false"
               enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="cn_activity_status" domainObjectName="ActivityStatus"
               enableUpdateByPrimaryKey="false"
               enableDeleteByPrimaryKey="false" enableSelectByPrimaryKey="false"
               enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="cn_note_activity" domainObjectName="NoteActivity"
               enableUpdateByPrimaryKey="false"
               enableDeleteByPrimaryKey="false" enableSelectByPrimaryKey="false"
               enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="cn_note_status" domainObjectName="NoteStatus"
               enableUpdateByPrimaryKey="false"
               enableDeleteByPrimaryKey="false" enableSelectByPrimaryKey="false"
               enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false"></table>
        <table tableName="cn_note_type" domainObjectName="NoteType"
               enableUpdateByPrimaryKey="false"
               enableDeleteByPrimaryKey="false" enableSelectByPrimaryKey="false"
               enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false"></table>
    </context>
</generatorConfiguration>
